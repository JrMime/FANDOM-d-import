{
  "name": "FANDOM Global Discussions JS & CSS Importer",
  "version": "1.0",
  "description": "Imports custom JavaScript and CSS into all Discussions on FANDOM",
  "content_scripts": [{
    "run_at" :"document_end",
    "matches": ["*://*.wikia.com/d*"],
    "js": ["import.js"]
  }],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "icons": {
    "16": "logo16.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "manifest_version": 2
}
